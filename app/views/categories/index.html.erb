<div class='page-header'>
  <%= link_to 'New Category', new_category_path, class:'btn btn-info pull-right' %>
  <h1>Categories associated with your website</h1>
</div>

<table class='table table-striped'>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
    <tr>
      <td><%= category.category_name %> </td>
    </tr>
    <% end %>
  </tbody>
</table>
